pkgname="anvl-getty"
pkgver=1.0
pkgrel=2
pkgdesc="Serial Getty for Anvl on ttyS2 or ttyO2"
arch=("armv7h")
url="https://github.com/networkimprov/arch-packages"
license=("none")
source=("anvl-getty.service")
md5sums=('22fcb27f5754ffe8f3b03cdafa7314ac')

package() {
  install -D -m 644 "${srcdir}/anvl-getty.service" \
    "${pkgdir}/etc/systemd/system/anvl-getty.service"
}

